

{
  "name": "ChromeLights",
  "description" : "Base Level Extension",
  "version": "0.1",
  "manifest_version": 2,
  "key": "",
  "oauth2": {
    "client_id": "http://851772945859-2c935hiq3ftnpu6a87btv8fjefhunm8f.apps.googleusercontent.com/",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "page_action": {
    "default_icon": "placeholder-icon.png",
    "default_title": "ChromeLights2"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "match_about_blank": true,
      "css": ["style.css"],
      "js": ["contentScript.bundle.js"]
      }
    ],
    "permissions": [
      "http://docs.sequelizejs.com/*",
      "activeTab",
      "tabs",
      "storage",
      "identity"
    ],
    "web_accessible_resources": [ "Raleway/*.ttf", "style.css" ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://apis.google.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}

