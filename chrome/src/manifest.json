{
  "name": "ChromeLights",
  "description" : "Base Level Extension",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "placeholder-icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.fullstackacademy.com/"],
      "css": ["style.css"]
    }
  ]
}
